[32m+[m[41m[m
[32m+[m[32m$param_module  = optional_param('module', null, PARAM_INT);  // module[m[41m[m
[32m+[m[32m$param_displaystart = optional_param('iDisplayStart', null, PARAM_INT);  // displaystart[m[41m[m
[32m+[m[32m$param_displaylength = optional_param('iDisplayLength', null, PARAM_INT);  // displaylength[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$aColumns = array( 'tu.name', 'cs.shortname', 'cs.fullname', 'sb.submission_filename', 'us.firstname', 'us.lastname', 'us.email', 'fl.filename', 'sb.submission_objectid' );[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$sQuery = 'SELECT[m[41m[m
[32m+[m[32m    fl.id AS id,[m[41m[m
[32m+[m[32m    cm.id AS cmid,[m[41m[m
[32m+[m[32m    tu.id AS activityid,[m[41m[m
[32m+[m[32m    tu.name AS activity,[m[41m[m
[32m+[m[32m    sb.submission_unanon AS unanon,[m[41m[m
[32m+[m[32m    us.firstname AS firstname,[m[41m[m
[32m+[m[32m    us.lastname AS lastname,[m[41m[m
[32m+[m[32m    us.email AS email,[m[41m[m
[32m+[m[32m    us.id AS userid,[m[41m[m
[32m+[m[32m    fl.mimetype AS mimetype,[m[41m[m
[32m+[m[32m    fl.filesize AS filesize,[m[41m[m
[32m+[m[32m    fl.timecreated AS created,[m[41m[m
[32m+[m[32m    fl.pathnamehash AS hash,[m[41m[m
[32m+[m[32m    fl.filename AS rawfilename,[m[41m[m
[32m+[m[32m    cs.fullname AS coursetitle,[m[41m[m
[32m+[m[32m    cs.shortname AS courseshort,[m[41m[m
[32m+[m[32m    cs.id AS course,[m[41m[m
[32m+[m[32m    sb.submission_filename AS filename,[m[41m[m
[32m+[m[32m    sb.submission_objectid AS objectid[m[41m[m
[32m+[m[32mFROM '.$CFG->prefix.'files fl[m[41m[m
[32m+[m[32mLEFT JOIN[m[41m[m
[32m+[m[32m    '.$CFG->prefix.'turnitintool_submissions sb ON fl.itemid = sb.id[m[41m[m
[32m+[m[32mLEFT JOIN[m[41m[m
[32m+[m[32m    '.$CFG->prefix.'user us ON fl.userid = us.id[m[41m[m
[32m+[m[32mLEFT JOIN[m[41m[m
[32m+[m[32m    '.$CFG->prefix.'course_modules cm ON fl.contextid = cm.id AND cm.module = '.$param_module.'[m[41m[m
[32m+[m[32mLEFT JOIN[m[41m[m
[32m+[m[32m    '.$CFG->prefix.'turnitintool tu ON cm.instance = tu.id[m[41m[m
[32m+[m[32mLEFT JOIN[m[41m[m
[32m+[m[32m    '.$CFG->prefix.'course cs ON tu.course = cs.id[m[41m[m
[32m+[m[32mWHERE[m[41m[m
[32m+[m[32m    fl.component = \'mod_turnitintool\' AND fl.filesize != 0';[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$sCountQuery = 'SELECT[m[41m[m
[32m+[m[32m    fl.id AS id[m[41m[m
[32m+[m[32mFROM[m[41m[m
[32m+[m[32m    '.$CFG->prefix.'files fl[m[41m[m
[32m+[m[32mLEFT JOIN[m[41m[m
[32m+[m[32m    '.$CFG->prefix.'turnitintool_submissions sb ON fl.itemid = sb.id[m[41m[m
[32m+[m[32mWHERE[m[41m[m
[32m+[m[32m    fl.component = "mod_turnitintool" AND fl.filesize != 0';[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$param_sortcol[0] = optional_param('iSortCol_0', null, PARAM_INT);  // sortcol[m[41m[m
[32m+[m[32m$param_sortingcols = optional_param('iSortingCols', 0, PARAM_INT);  // sortingcols[m[41m[m
[32m+[m[32m$sOrder = "";[m[41m[m
[32m+[m[32mif ( !is_null( $param_sortcol[0] ) ) {[m[41m[m
[32m+[m[32m    $sOrder = " ORDER BY ";[m[41m[m
[32m+[m[32m    $startOrder = $sOrder;[m[41m[m
[32m+[m[32m    for ( $i=0; $i < intval( $param_sortingcols ); $i++ ) {[m[41m[m
[32m+[m[32m        $param_sortcol[$i] = optional_param('iSortCol_'.$i, null, PARAM_INT);  // sortcol[m[41m[m
[32m+[m[32m        $param_sortable[$i] = optional_param('bSortable_'.$param_sortcol[$i], null, PARAM_TEXT);  // sortable[m[41m[m
[32m+[m[32m        $param_sortdir[$i] = optional_param('sSortDir_'.$i, null, PARAM_TEXT);  // sortdir[m[41m[m
[32m+[m[32m        if ( $param_sortable[$i] == "true" ) {[m[41m[m
[32m+[m[32m            $sOrder .= $aColumns[ $param_sortcol[$i] ] . " " . $param_sortdir[$i] . ", ";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    $sOrder = substr_replace( $sOrder, "", -2 );[m[41m[m
[32m+[m[32m    if ( $sOrder == $startOrder ) $sOrder = "";[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[32m$sOrder .= ",tu.id asc ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$param_search = optional_param('sSearch', null, PARAM_TEXT);  // sortingcols[m[41m[m
[32m+[m[32m$start = true;[m[41m[m
[32m+[m[32m$sWhere = ' AND ( ';[m[41m[m
[32m+[m[32m$nobracket = false;[m[41m[m
[32m+[m[32mfor ( $i=0; $i < count($aColumns); $i++ ) {[m[41m[m
[32m+[m[32m    $param_searchable[$i] = optional_param('bSearchable_'.$i, null, PARAM_TEXT);[m[41m[m
[32m+[m[32m    $param_search_n[$i] = optional_param('sSearch_'.$i, null, PARAM_TEXT);[m[41m[m
[32m+[m[32m    if ( !is_null($param_searchable[$i]) && $param_searchable[$i] == "true" && ( $param_search != '' OR $param_search_n[$i] != '' ) ) {[m[41m[m
[32m+[m[32m        if ( !$start ) $sWhere .= " OR ";[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        if ( $aColumns[$i] == 'sb.submission_objectid' AND $param_search_n[$i] == '##deletable##' ) {[m[41m[m
[32m+[m[32m            $sWhere = ( $sWhere == ' AND ( ' ) ? '' : substr_replace( $sWhere, "", -3 ) . ' )';[m[41m[m
[32m+[m[32m            $sWhere .= " AND ( sb.submission_objectid IS NOT NULL OR sb.submission_filename IS NULL )";[m[41m[m
[32m+[m[32m            $nobracket = true;[m[41m[m
[32m+[m[32m        } else if ( $aColumns[$i] != ' ' ) {[m[41m[m
[32m+[m[32m            $sWhere .= "CAST(" . $aColumns[$i] . " AS CHAR) LIKE '%" . $param_search . "%'";[m[41m[m
[32m+[m[32m            $start = false;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[32mif ( $sWhere != ' AND ( ' AND !$nobracket ) {[m[41m[m
[32m+[m[32m    $sWhere .= " ) ";[m[41m[m
[32m+[m[32m} else if ( $nobracket ) {[m[41m[m
[32m+[m[32m    $sWhere .= " ";[m[41m[m
[32m+[m[32m} else {[m[41m[m
[32m+[m[32m    $sWhere = "";[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$sLimit = "";[m[41m[m
[32m+[m[32mif ( !is_null( $param_displaystart ) && $param_displaylength != '-1' ) {[m[41m[m
[32m+[m[32m    $limitfrom = $param_displaystart;[m[41m[m
[32m+[m[32m    $limitnum  = $param_displaylength;[m[41m[m
[32m+[m[32m} else {[m[41m[m
[32m+[m[32m    $limitfrom = ( isset($DB) AND is_callable(array($DB,'get_records_sql')) ) ? 0 : '';[m[41m[m
[32m+[m[32m    $limitnum  = ( isset($DB) AND is_callable(array($DB,'get_records_sql')) ) ? 0 : '';[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$sQuery .= $sWhere;[m[41m[m
[32m+[m[32m$cResult = ( isset($DB) AND is_callable(array($DB,'get_records_sql')) ) ? $DB->get_records_sql( $sQuery, array() ) : get_records_sql( $sQuery );[m[41m[m
[32m+[m[32m$iTotal = count( $cResult );[m[41m[m
[32m+[m[32m$sQuery .= $sOrder . $sLimit;[m[41m[m
[32m+[m[32m$rResult = ( isset($DB) AND is_callable(array($DB,'get_records_sql')) )[m[41m[m
[32m+[m[32m                ? $DB->get_records_sql( $sQuery, array(), $limitfrom, $limitnum )[m[41m[m
[32m+[m[32m                : get_records_sql( $sQuery, $limitfrom, $limitnum  );[m[41m[m
[32m+[m[32m$iFilteredTotal = $iTotal; //count( $rResult );[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$param_echo = optional_param('sEcho', 0, PARAM_INT);  // echo[m[41m[m
[32m+[m[32m$output = array([m[41m[m
[32m+[m[32m    "sEcho" => $param_echo,[m[41m[m
[32m+[m[32m    "iTotalRecords" => $iTotal,[m[41m[m
[32m+[m[32m    "iTotalDisplayRecords" => $iFilteredTotal,[m[41m[m
[32m+[m[32m    "aaData" => array()[m[41m[m
[32m+[m[32m);[m[41m[m
[32m+[m[41m[m
[32m+[m[32mforeach ( $rResult as $result ) {[m[41m[m
[32m+[m[32m    // 'cs.fullname', 'tu.name', 'fl.filename', 'us.firstname', 'us.lastname', 'us.email', 'fl.timecreated'[m[41m[m
[32m+[m[32m    if ( !empty( $param_search ) AND !is_null( $result->unanon ) AND !$result->unanon ) {[m[41m[m
[32m+[m[32m        $output['iTotalDisplayRecords'] = $output['iTotalDisplayRecords'] - 1;[m[41m[m
[32m+[m[32m        continue; // If these are search results and this is anonymised skip it[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    $row = array();[m[41m[m
[32m+[m[32m    if ( is_null( $result->filename ) OR is_null( $result->activityid ) ) {[m[41m[m
[32m+[m[32m        $row[] = '<i><b>'.get_string('assigngeterror','turnitintool').'</b></i>';[m[41m[m
[32m+[m[32m    } else {[m[41m[m
[32m+[m[32m        $row[] = '<a href="'.$CFG->wwwroot.'/mod/turnitintool/view.php?id='.$result->cmid.'&do=allsubmissions">' . $result->coursetitle . ' (' . $result->courseshort . ') - ' . $result->activity . '</a>';[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    $row[] = $result->courseshort;[m[41m[m
[32m+[m[32m    $row[] = $result->coursetitle;[m[41m[m
[32m+[m[32m    $row[] = '<a href="'.$CFG->wwwroot.'/mod/turnitintool/extras.php?do=files&fileid='.$result->id.'&filehash='.$result->hash.'" class="fileicon">'[m[41m[m
[32m+[m[32m                . ( is_null( $result->filename ) ? $result->rawfilename : $result->filename ) . '</a>';[m[41m[m
[32m+[m[32m    $row[] = ' ';[m[41m[m
[32m+[m[32m    $row[] = ( !is_null( $result->unanon ) AND !$result->unanon ) ? get_string( 'anonenabled', 'turnitintool' ) : '<a href="'.$CFG->wwwroot.'/user/view.php?id='.$result->userid.'">'[m[41m[m
[32m+[m[32m              .$result->lastname . ', ' . $result->firstname . '</a> (' . $result->email . ')';[m[41m[m
[32m+[m[32m    $row[] = ' ';[m[41m[m
[32m+[m[32m    $row[] = userdate( $result->created );[m[41m[m
[32m+[m[32m    $fnd = array("\n","\r");[m[41m[m
[32m+[m[32m    $rep = array('\n','\r');[m[41m[m
[32m+[m[32m    $row[] = ( !is_null( $result->objectid ) OR is_null( $result->filename ) ) ? '<a href="'.$CFG->wwwroot.'/mod/turnitintool/extras.php?do=files&fileid='[m[41m[m
[32m+[m[32m           .$result->id.'&filehash='.$result->hash.'&filerem=true" '[m[41m[m
[32m+[m[32m           .' onclick="return confirm(\''.str_replace($fnd, $rep, get_string('filedeleteconfirm','turnitintool')).'\')">'[m[41m[m
[32m+[m[32m           .'<img src="pix/delete.png" class="tiiicons" alt="'.get_string('delete','turnitintool').'" /></a>' : '';[m[41m[m
[32m+[m[32m    $output['aaData'][] = $row;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m$output['query']=$sQuery;[m[41m[m
[32m+[m[41m[m
 echo json_encode( $output );[m
